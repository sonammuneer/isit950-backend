generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
  binaryTargets   = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          Int    @id @default(autoincrement())
  email       String @unique
  password    String
  name        String
  phonenumber String
  dateofbirth String
}

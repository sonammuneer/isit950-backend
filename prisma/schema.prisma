generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
  binaryTargets   = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          String @id @default(uuid())
  email       String @unique
  password    String
  name        String
  phonenumber String
  dateofbirth String
}

model Hotel {
  id         String @id @default(uuid())
  name       String @unique
  place      String
  adminemail String
}
